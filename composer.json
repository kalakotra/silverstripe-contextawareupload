{
    "name": "symbiote/silverstripe-contextawareupload",
    "description": "Automatic organization of uploaded files based on the site tree structure.",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "symbiote",
        "context",
        "aware",
        "upload"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Symbiote",
            "homepage": "https://www.symbiote.com.au/"
        },
        {
            "name": "Jake Bentvelzen",
            "email": "jake@symbiote.com.au"
        }
    ],
    "require": {
        "silverstripe/framework": "^5.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": "^5.7"
    },
    "scripts": {
        "phpcbf": "phpcbf src/ tests/",
        "phpcs": "phpcs src/ tests/"
    },
    "autoload": {
        "psr-4": {
            "Symbiote\\ContextAwareUpload\\": "src/",
            "Symbiote\\ContextAwareUpload\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    },
	"replace": {
		"silbinarywolf/silverstripe-contextawareupload": "self.version"
	},
    "prefer-stable": true,
    "minimum-stability": "dev"
}
